  "version": "1.0.1",
  "homepage": "https://github.com/oliverswitzer/extelenium",
  "author": "Oliver Switzer (http://github.com/oliverswitzer)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliverswitzer/extelenium"
  },
  "main": "scripts/extelenium.js",
  "scripts": {
